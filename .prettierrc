{
  "printWidth": 120,
  "tabWidth": 2,
  "tabs": false,
  "semi": true,
  "quotes": false,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "bracketSpacing": true,
  "objectWrap": "preserve",
  "bracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "singleAttributePerLine": false
}
